{
  "name": "myownportfolio",
  "version": "1.0.0",
  "description": "Permet la création rapide d'un portfolio à partir de modules ReactJS personnalisés",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "documentation": "node etc/scripts/generate_doc.js && browserify docs/json_schema/script.js -o docs/json_schema/bundle.js",
    "init": "bash etc/scripts/init.sh",
    "compile": "node etc/scripts/generate_modules_list.js",
    "sass": "sass app/index.scss:dist/style/index.css -t compressed -r sass-json-vars --sourcemap=none",
    "webpack": "webpack --config webpack.config.js",
    "webpack:prod": "webpack --config webpack.config.js --env.production",
    "build": "npm run init && npm run compile && npm run sass && npm run webpack",
    "build:prod": "npm run init && npm run compile && npm run sass && npm run webpack:prod",
    "start": "node etc/scripts/express.js",
    "clean": "bash etc/scripts/clean.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacBootglass/myOwnPortfolio.git"
  },
  "author": "thibault.theologien@insa-rouen.fr",
  "license": "ISC",
  "dependencies": {
    "Timelined": "^0.2.3",
    "ajv": "^5.0.1-beta.3",
    "aos": "^2.1.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "bootstrap": "^4.0.0-alpha.6",
    "docson": "^1.0.2",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.0",
    "gulp-exec": "^2.1.3",
    "gulp-htmlmin": "^3.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "^2.1.1",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^3.0.0",
    "headroom.js": "^0.9.3",
    "json-loader": "^0.5.4",
    "opener": "^1.4.3",
    "particlesjs": "^2.0.1",
    "prop-types": "^15.6.0",
    "rc-progress": "^2.1.0",
    "react": "^15.6.2",
    "react-d3-map": "^0.8.3",
    "react-dom": "^15.6.2",
    "react-helmet": "^5.0.3",
    "react-markdown": "^2.4.6",
    "react-on-visible": "^1.1.0",
    "react-rotating-text": "^1.1.3",
    "react-tap-event-plugin": "^2.0.1",
    "slug": "^0.9.1",
    "webpack": "^3.6.0",
    "webpack-stream": "^3.2.0"
  },
  "bugs": {
    "url": "https://github.com/MacBootglass/myOwnPortfolio/issues"
  },
  "homepage": "https://github.com/MacBootglass/myOwnPortfolio#readme",
  "devDependencies": {
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0"
  }
}
